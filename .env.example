# EXLO Development Environment Variables
#
# Copy this file to .env and fill in your actual values.
# Never commit .env to version control!
#
# This file is automatically loaded by sbt-dotenv when running tests.

# NewsAPI API Key (get free key at https://newsapi.org/register)
# Required for running NewsAPI integration tests
NEWSAPI_API_KEY=your-api-key-here

# Connector configuration (JSON format)
# This is what the YAML connector reads for config interpolation
EXLO_CONNECTOR_CONFIG={"api_key":"your-api-key-here","country":"us","category":"general"}
